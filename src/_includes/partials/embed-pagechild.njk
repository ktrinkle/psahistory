<div class="container">
    <div class="row row-cols-1 row-cols-sm-2 row-cols-md-3">
        {%- for subpage in collections[collectionName] -%}
        <div class="col">
            <a href="/{{ subpage.data.permalink }}" class="link-underline link-underline-opacity-0 link-psa-dark-red">
                <h3 class="text-start instrument-serif">{{ subpage.data.title }}</h3>
                <p class="text-start">{{ subpage.data.meta_desc }}</p>
            </a>
        </div>
        {%- endfor -%}
    </div>
</div> 
